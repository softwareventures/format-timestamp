{
  "private": true,
  "name": "@softwareventures/format-timestamp",
  "version": "0.0.0-development",
  "description": "Convert @softwareventures/timestamp to a string in a variety of formats",
  "keywords": [
    "timestamp",
    "date",
    "time",
    "format",
    "text",
    "string",
    "TypeScript"
  ],
  "author": "Daniel Cassidy <mail@danielcassidy.me.uk>",
  "homepage": "https://github.com/softwareventures/format-timestamp",
  "bugs": "https://github.com/softwareventures/format-timestamp/issues",
  "repository": "github:softwareventures/format-timestamp",
  "license": "ISC",
  "scripts": {
    "fix": "eslint . --fix && prettier --write .",
    "lint": "eslint . && prettier --check .",
    "prepare": "tsc",
    "semantic-release": "semantic-release",
    "test": "ava"
  },
  "dependencies": {
    "@softwareventures/array": "^3.10.0",
    "@softwareventures/format-date": "^1.0.0-alpha.1",
    "@softwareventures/timestamp": "^1.0.0-alpha.4",
    "tslib": "2.3.0"
  },
  "devDependencies": {
    "@softwareventures/eslint-config": "3.6.2",
    "@softwareventures/prettier-config": "1.0.2",
    "@softwareventures/semantic-release-config": "1.1.1",
    "@softwareventures/tsconfig": "5.1.0",
    "ava": "3.15.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.29.0",
    "prettier": "2.3.1",
    "semantic-release": "17.4.4",
    "ts-node": "9.1.1",
    "typescript": "4.3.4"
  },
  "eslintConfig": {
    "root": true,
    "extends": "@softwareventures"
  },
  "prettier": "@softwareventures/prettier-config",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register/files"
    ]
  },
  "release": {
    "extends": "@softwareventures/semantic-release-config"
  }
}
